{
  "env": {
    "node": true,
    "browser": true
  },
  "globals": {
    "console": true
  },
  "rules": {
    "indent": [2, 2],
    "no-unused-vars": [2, {
      "vars": "all",
      "args": "after-used"
    }],
    "brace-style": [2, "1tbs"],
    "camelcase": [2, {
      "properties": "never"
    }],
    "comma-style": [2, "last"],
    "default-case": 2,
    "func-style": [2, "declaration"],
    "guard-for-in": 2,
    "no-floating-decimal": 2,
    "no-nested-ternary": 2,
    "no-undefined": 2,
    "radix": 2,
    "space-after-function-name": [2, "never"],
    "space-after-keywords": [2, "always"],
    "space-before-blocks": 2,
    "spaced-line-comment": [2, "always", {
      "exceptions": ["-"]
    }],
    "no-alert": 0,
    "strict": [2, "global"],
    "wrap-iife": 2,
    "consistent-this": [0, "me"]
  }
}
